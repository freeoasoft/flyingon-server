{

    //http配置
    "http": {
        "port": 8085,
        "logon": "/logon" //登录页地址
    },

    //session配置
    "session": {
        "timeout": 20,       //session过期时间(分钟)
        "cache": 1000,       //本地缓存最大数量
        "type": "redis",     //使用redis缓存作为session保存方式
        "redis": {           //redis配置
            "host": "127.0.0.1",
            "port": 6379,
            "options": {}
        }
    }

}